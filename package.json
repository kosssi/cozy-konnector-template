{
  "name": "cozy-konnector-template",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konnectors/cozy-konnector-template.git"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0",
  "main": "./src/index.js",
  "eslintConfig": {
    "extends": [
      "eslint-config-cozy-app"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "cozy-konnector-dev src/index",
    "standalone": "cozy-konnector-standalone src/index",
    "build": "webpack",
    "precommit": "yarn lint",
    "lint": "eslint --fix .",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/konnectors/cozy-konnector-template.git}"
  },
  "dependencies": {
    "cozy-konnector-libs": "3.7.4"
  },
  "devDependencies": {
    "copy-webpack-plugin": "4.5.1",
    "cozy-jobs-cli": "1.0.6",
    "eslint": "4.19.1",
    "eslint-config-cozy-app": "0.5.1",
    "git-directory-deploy": "1.5.1",
    "husky": "0.14.3",
    "webpack": "4.2.0",
    "webpack-cli": "2.0.13"
  }
}
